{
    "extends": "airbnb-base",
    "rules": {
        "indent": [2, 4, { "SwitchCase": 1 }],
        "max-len": ["error", 120, 2, {
            "ignoreUrls": true,
            "ignoreComments": false,
            "ignoreRegExpLiterals": true,
            "ignoreStrings": true,
            "ignoreTemplateLiterals": true
        }]
    }
}
